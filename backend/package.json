{
    "name": "attendanzy-backend",
    "version": "1.0.0",
    "description": "Backend server for Attendanzy - Attendance Management System",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --coverage --detectOpenHandles",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "audit": "npm audit --audit-level=moderate",
        "audit:fix": "npm audit fix",
        "security:check": "npm audit && npx snyk test",
        "docker:build": "docker build -t attendanzy-backend .",
        "docker:run": "docker run -p 5000:5000 --env-file .env attendanzy-backend"
    },
    "keywords": [
        "attendance",
        "management",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.5.1",
        "express-validator": "^7.0.1",
        "firebase-admin": "^13.6.0",
        "helmet": "^7.2.0",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.18.0",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "eslint": "^8.57.1",
        "eslint-plugin-security": "^2.1.1",
        "jest": "^29.7.0",
        "nodemon": "^3.1.11",
        "supertest": "^6.3.4"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
